FROM mcr.microsoft.com/dotnet/runtime:7.0 AS base
WORKDIR /app

FROM mcr.microsoft.com/dotnet/sdk:7.0 AS build
WORKDIR /src
COPY ["Enzyme/Enzyme.csproj", "Enzyme/"]
RUN dotnet restore "Enzyme/Enzyme.csproj"
COPY . .
WORKDIR "/src/Enzyme"
RUN dotnet build "Enzyme.csproj" -c Release -o /app/build

FROM build AS publish
RUN dotnet publish "Enzyme.csproj" -c Release -o /app/publish /p:UseAppHost=false

FROM base AS final
WORKDIR /app
COPY --from=publish /app/publish .
ENTRYPOINT ["dotnet", "Enzyme.dll"]
